{
  "name": "keye-dev.github.io",
  "homepage": "https://www.keye.dev",
  "private": true,
  "version": "0.1.0",
  "engines": {
    "node": ">=16.14.0"
  },
  "keywords": [
    "key",
    "keye",
    "auth",
    "authentication",
    "authorization"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "lint": "eslint --ext .js,.ts,.vue --ignore-path .gitignore --fix src",
    "format": "prettier --write .",
    "build": "vue-tsc --noEmit && vite build",
    "deploy": "cp CNAME dist/CNAME && scripts/deploy.sh",
    "dev": "vite",
    "preview": "vite preview"
  },
  "dependencies": {
    "vue": "^3.2.31"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.1.0",
    "@vitejs/plugin-vue": "^2.2.0",
    "@vue/eslint-config-prettier": "^7.0.0",
    "@vue/eslint-config-typescript": "^10.0.0",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-vue": "^8.4.1",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5",
    "vite": "^2.8.3",
    "vite-plugin-eslint": "^1.3.0",
    "vue-tsc": "^0.29.8"
  },
  "simple-git-hooks": {
    "pre-commit": "pnpm exec lint-staged --concurrent false",
    "commit-msg": "pnpm exec ts-node scripts/verifyCommit.ts $1"
  },
  "lint-staged": {
    "*": [
      "prettier --write --ignore-unknown"
    ],
    "packages/*/{src,types}/**/{*.js,*.ts}": [
      "eslint --ext .js,.ts"
    ],
    "packages/**/{*.d.js,*.d.ts}": [
      "eslint --ext .js,.ts"
    ],
    "src": [
      "eslint --ext .js,.ts,.vue --ignore-path .gitignore"
    ]
  },
  "packageManager": "pnpm@6.31.0"
}
